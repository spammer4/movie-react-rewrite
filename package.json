{
  "name": "movie-typescript",
  "version": "1.0.0",
  "description": "Rewrite of the react rocks movie page in typescript. Reference URL https://skempin.github.io/reactjs-tmdb-app/. Git hub link https://github.com/SKempin/reactjs-tmdb-app",
  "main": "index.js",
  "scripts": {
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && webpack",
    "start": "npm run prettier:write && webpack-dev-server --mode development --open",
    "clean": "rimraf dist",
    "analyse-bundle": "npm run clean && webpack --env.ANALYSE_BUNDLES=yes"
  },
